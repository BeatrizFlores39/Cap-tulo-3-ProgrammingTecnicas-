#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;
    int count = 0;
    for (int r1 = 0; r1 < n; r1++) {
        for (int c1 = 0; c1 < n; c1++) {
            for (int r2 = 0; r2 < n; r2++) {
                for (int c2 = 0; c2 < n; c2++) {
                    if (r1 == r2 && c1 == c2) continue;
                    if (r1 == r2 || c1 == c2) continue;
                    if (abs(r1 - r2) == abs(c1 - c2)) continue;
                    count++;
                }
            }
        }
    }
    count /= 2; // Cada par contado dos veces
    cout << count << "\n";
    return 0;
}

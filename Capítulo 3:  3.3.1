#include <bits/stdc++.h>
using namespace std;

int main() {
    int n, x;
    cin >> n >> x;
    vector<int> a(n);
    for (int i = 0; i < n; i++) cin >> a[i];

    bool ok1 = false;
    for (int i = 0; i < n; i++) {
        if (a[i] == x) ok1 = true;
    }

    bool ok2 = false;
    for (int i = 0; i < n; i++) {
        if (a[i] == x) {
            ok2 = true;
            break;
        }
    }

    cout << "Versión 1: " << (ok1 ? "Encontrado" : "No encontrado") << "\n";
    cout << "Versión 2: " << (ok2 ? "Encontrado" : "No encontrado") << "\n";

    return 0;
}
